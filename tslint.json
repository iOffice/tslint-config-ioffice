{
  "rulesDirectory": "node_modules/tslint-eslint-rules/dist/rules",
  "rules": {
    "ter-indent": [true, 2, {
      "SwitchCase": 1,
      "outerIIFEBody": 0,
      "MemberExpression": 1,
      "FunctionDeclaration": { "body": 1, "parameters": 1 },
      "FunctionExpression": { "body": 1, "parameters": 1 },
      "CallExpression": { "arguments": 1 }
    }]
  }
}
